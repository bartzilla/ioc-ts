{
  "name": "ioc_ts",
  "version": "1.0.0",
  "description": "This is IoC demo using TypeScript",
  "main": "index.js",
  "scripts": {
    "test": "nyc _mocha server/test/**/*.ts",
    "cover": "istanbul cover _mocha server/**/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "server/**/*.ts",
      "server/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "Cipriano",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.2",
    "inversify": "^3.1.0",
    "reflect-metadata": "^0.1.10",
    "typescript": "^2.2.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.7",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^10.1.2",
    "rewire": "^2.5.2",
    "sinon": "^1.17.7",
    "source-map-support": "^0.4.14",
    "ts-node": "^2.1.0"
  }
}
